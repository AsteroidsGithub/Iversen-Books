{
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "importOrder": [
    "^@Views/(.*)$",
    "^@Components/(.*)$",
    "^@Services/(.*)$",
    "^@Middleware/(.*)$",
    "^@Utilities/(.*)$",
    "^@Interfaces/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
